{
    "name": "stukenov/nova-tiptap",
    "description": "A Laravel Nova tiptap editor field.",
    "keywords": [
        "laravel",
        "nova",
        "tiptap",
        "vue",
        "editor"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.0|^7.3"
    },
    "require-dev": {
        "laravel/nova": "^3.0",
        "orchestra/testbench": "^7.3.0",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "STukenov\\Tiptap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STukenov\\Tiptap\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
